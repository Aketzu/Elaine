  <p>
    <%= f.label :pms_id %><br />
    <%= f.text_field :pms_id %>
  </p>
  <p>
    <%= f.label :preview_image_offset %><br />
    <%= f.text_field :preview_image_offset %>
  </p>
  <p>
    <%= f.label :do_vod %><br />
    <%= f.check_box :do_vod %>
  </p>
  <p>
    <%= f.label :hidden %><br />
    <%= f.check_box :hidden %>
  </p>
  <p>
